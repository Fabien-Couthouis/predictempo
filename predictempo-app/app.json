{
  "expo": {
    "name": "predictempo-app",
    "slug": "predictempo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/predictempo_icon_color.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/predictempo_icon_color.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.predictempo.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/predictempo_icon_color.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/predictempo_icon_color.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "94b70d6d-eeeb-498d-924a-102ad57a2bea"
      }
    }
  }
}
